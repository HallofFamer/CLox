namespace test.features;

fun fibonacci(){ 
    var last = 0;
    var current = yield 1;

    while(true) { 
        current = last + current;
        last = current - last;
        yield current;
    }
}

val generator = fibonacci();
println(generator);
var i = 1;
while(i <= 10) {
    generator.next();
    println("Fibonacci number at index ${i}: ${generator.current}");
    i = i + 1;
}

namespace test.features;

fun fibonacci(){ 
    var last = 0;
    var current = 1;
    yield current;

    while(true) { 
        current = last + current;
        last = current - last;
        yield current;
    }
}

val generator = fibonacci();
println(generator);
var i = 1;
var num = 0;
while(i <= 15) {
    println("Fibonacci number at index ${i}: ${generator.next().current}");
    i = i + 1;
}

namespace test.features;

fun fibonacci(){ 
    var last = 0;
    var current = yield 1;
    var next = 1;

    while(true) { 
        current = last + current;
        last = current - last;
        next = yield current;
    }
}

val generator = fibonacci();
println("Creating generator: ${generator}");
var i = 1;
while(i <= 10) {
    println("Fibonacci number at index ${i}: ${generator.next().value}");
    i = i + 1;
}

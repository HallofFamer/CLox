var list = List();
print("Creating empty list: ");
println(list);
var list2 = List();
print("Two empty lists are equal: ");
println(list.equals(list2));

print("Adding elements 2, 3.5, true and 'Hello World': ");
list.add(2);
list.add(3.5);
list.add(true);
list.add("Hello World");
println(list);
print("Testing two list equality again: ");
println(list.equals(list2));

print("Getting element at index 3: ");
println(list.getAt(3));
print("Index of element 'true': ");
println(list.indexOf(true));

print("Setting element false at index 2: ");
list.putAt(2, false);
println(list.toString());
print("Inserting element nil at index 3: ");
list.insertAt(3, nil);
println(list.toString());

print("Remove element at index 1: ");
var element = list.removeAt(1);
println(element.toString());
print("Current List is: ");
println(list.toString());
print("Clearing List: ");
list.clear();
println(list.toString());
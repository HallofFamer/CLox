var range = 0..4;
println("Creating an instance of Range class: (" + range.from().toString() + ".." +range.to().toString() + ")");
print("Length of Range: ");
println(range.length());
print("Range contains value 2 :");
println(range.contains(2));
println("");

println("Looping through Range: ");
for(var n : range){ 
    println(n);
}
println("");

print("Convert Range to Array: ");
println(range.toArray());
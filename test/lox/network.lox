using clox.std.network.IPAddress;

val ip = IPAddress("127.0.0.1");
println("IP Address: " + ip.toString());
println("IP is v4: " + ip.isIPV4().toString());
print("IP to array: ");
println(ip.toArray());

val ip2 = IPAddress("2001:0db8:85a3:0000:0000:8a2e:0370:7334");
println("IP2 Address: " + ip2.toString());
println("IP2 is v6: " + ip2.isIPV6().toString());
print("IP2 to array: ");
println(ip2.toArray());
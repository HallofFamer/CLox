class Zoo {
  init() {
    this.aardvark = 1;
    this.baboon   = 1;
    this.cat      = 1;
    this.donkey   = 1;
    this.elephant = 1;
    this.fox      = 1;
  }
  ant()    { return this.aardvark; }
  banana() { return this.baboon; }
  tuna()   { return this.cat; }
  hay()    { return this.donkey; }
  grass()  { return this.elephant; }
  mouse()  { return this.fox; }
}

val zoo = Zoo();
//zoo.aardvark = 1;

var sum = 0;
val start = clock();
while (sum < 10000000) { 
    sum = sum + zoo.aardvark + zoo.baboon + zoo.cat + zoo.donkey + zoo.elephant + zoo.fox;
}

print("Time taken for property access: ");
print(clock() - start);
println(" seconds");
println("Sum: " + sum.toString());
println("");

var sum2 = 0;
val start2 = clock();
while (sum2 < 10000000) {
    sum2 = sum2 + zoo.ant() + zoo.banana() + zoo.tuna() + zoo.hay() + zoo.grass() + zoo.mouse();
}

print("Time taken for method calls: ");
print(clock() - start2);
println(" seconds");
println("Sum: " + sum2.toString());
println("");
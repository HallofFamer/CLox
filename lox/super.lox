class A { 
    method(){ 
        print "A method";
    }

    toString(){ 
        return super.toString() + " of class " + this.getClassName();
    }
}

class B < A { 
    method(){ 
        print "B method";
    }

    test(){
        super.method();
    }
}

class C < B { }

var a = A();
var b = B();
var c = C();
c.test();

print(c.toString());
print(c.instanceOf(C));
print(c.instanceOf(B));
print(c.instanceOf(A));
print(c.instanceOf(Object));
print(b.instanceOf(C));
print(b.instanceOf(A));
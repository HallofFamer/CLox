class A { 
    method(){ 
        print "A method";
    }

    toString(){ 
        return super.toString() + " of class " + this.getClassName();
    }
}

class B < A { 
    method(){ 
        print "B method";
    }

    test(){
        super.method();
    }
}

class C < B { }

var c = C();
c.test();
print(c.toString());